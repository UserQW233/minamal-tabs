{
  "manifest_version": 3,
  "name": "Мои закладки",
  "version": "1.0",
  "description": "Отображает закладки пользователя на стартовой странице новой вкладки.",
  "permissions": ["bookmarks"],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
